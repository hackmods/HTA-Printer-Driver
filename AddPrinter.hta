					End If
			
					If SE182Colour.Checked = True Then
						objNetwork.AddWindowsPrinterConnection wellandUNCpath & prntwelse1821
					End If
				End If
End If

'********************************
' NOTL Library Checkboxes
'********************************
If NOTLlibrary.Style.Display = "inline" Then
